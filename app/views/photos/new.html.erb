<div class="container">
  <%= form_for @photo, :html => { :multipart => true } do |f| %>
    <p>
      <%= f.label :title %><br />
     <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :description %><br />
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.file_field :photo %>
    </p>
    <p><%= f.submit "Submit" %></p>
  <% end %>
</div>